<operators>
  
  <operator name="string.contains">
    <type>str</type>
    <template>$(F) like '%$(V)%'</template>
  </operator>
  
  <operator name="string.startsWith">
    <type>str</type>
    <template>$(F) like '$(V)%'</template>
  </operator>

  <operator name="string.endsWith">
    <type>str</type>
    <template>$(F) like '%$(V)'</template>
  </operator>

  <operator name="string.equals">
    <type>str</type>
    <template>$(F) = '$(V)'</template>
  </operator>

  <operator name="string.matches">
    <type>str</type>
    <template>$(F) like '$(V)'</template>
  </operator>
      
</operators>
